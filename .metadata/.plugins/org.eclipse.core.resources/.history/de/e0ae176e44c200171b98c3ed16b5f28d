#ifndef GPS_H_
#define GPS_H_

struct GPS_Init_d {
	UART_HandleTypeDef gpsUART;
};

struct GPS_Data_d {
	float latitude;
	float longitude;
	float altitude;
	char* date;
};
/*
 * Init struct.
 */
struct GPS_Init_d Init(UART_HandleTypeDef gpsUART);
/*
 * Initalize display.
 */
void GPS_Init(struct GPS_Init_d gps_d);
/*
 *
 */
GPS_d getData(struct GPS_Init_d gps_d);

#endif /* GPS_H_ */
