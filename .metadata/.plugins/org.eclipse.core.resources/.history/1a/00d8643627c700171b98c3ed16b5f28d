#ifndef GPS_H_
#define GPS_H_
#include "stm32l4xx_hal.h"


struct GPS_Init_d {
	UART_HandleTypeDef gpsUART;
};

struct GPS_Data_d {
	char* latitude;
	char* longitude;
	char* date;
};
/*
 * Init struct.
 */
struct GPS_Init_d InitGPS(UART_HandleTypeDef gpsUART);
/*
 * Get data.
 */
struct GPS_Data_d getData(struct GPS_Init_d gps_init_d);
/**
 *
 */
char* readString(struct GPS_Init_d gps_init_d);

#endif /* GPS_H_ */
