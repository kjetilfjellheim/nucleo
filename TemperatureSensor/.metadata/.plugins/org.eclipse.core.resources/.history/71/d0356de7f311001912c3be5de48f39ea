#ifndef SSD1306_TYPES_H_
#define SSD1306_TYPES_H_

typedef struct {
	uint8_t width;
	uint8_t height;
	uint8_t inverted;
	uint8_t initialized;
	uint8_t ssd1306Buffer[];
	I2C_HandleTypeDef ssd1306;
} SSD1306_t;

#endif /* SSD1306_TYPES_H_ */
